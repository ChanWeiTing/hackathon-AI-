#openai.api_key = os.getenv("OPENAI_API_KEY")

import openai
openai.api_key = "key" #enter openai api key here

def create_image(prompt):
  response = openai.images.generate(
    model="dall-e-2",
    prompt=prompt,
    size="512x512",
    quality="standard",
    n=1,
  )
  image_url= response.data[0].url
  return image_url
