import openai
openai.api_key = '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'

# Change the prompt to generate the image you want
response = openai.images.generate(
  model="dall-e-2",
  prompt="orange cat with a hat",
  size="512x512",
  quality="standard",
  n=1,
)

image_url = response.data[0].url
print(image_url)
