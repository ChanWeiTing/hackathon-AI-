import openai
openai.api_key = "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"

def create_image(prompt):
  response = openai.images.generate(
    model="dall-e-2",
    prompt=prompt,
    size="512x512",
    quality="standard",
    n=1,
  )
  image_url= response.data[0].url
  return image_url
